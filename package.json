{
  "name": "resource-bundle-version-merger",
  "version": "1.0.7",
  "description": "A Resource Bundle Version Merger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "run": "node ./lib/index.js",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "rbvm": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisabovo/ResourceBundleVersionMerger.git"
  },
  "keywords": [
    "Resource",
    "Bundle",
    "Version",
    "Merger",
    "JS",
    "Properties"
  ],
  "author": "chrisabovo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisabovo/ResourceBundleVersionMerger/issues"
  },
  "homepage": "https://github.com/chrisabovo/ResourceBundleVersionMerger#readme",
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "fs": "0.0.1-security",
    "load-json-file": "^5.1.0",
    "lodash": "^4.17.11"
  }
}
