{
  "name": "resource-bundle-version-merger",
  "version": "1.0.10",
  "description": "A Resource Bundle Version Merger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisabovo/ResourceBundleVersionMerger.git"
  },
  "keywords": [
    "Resource",
    "Bundle",
    "Version",
    "Merger",
    "JS",
    "Properties"
  ],
  "author": "chrisabovo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisabovo/ResourceBundleVersionMerger/issues"
  },
  "homepage": "https://github.com/chrisabovo/ResourceBundleVersionMerger#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "commander": "^3.0.1",
    "fs": "0.0.2",
    "load-json-file": "^6.2.0",
    "lodash": "^4.17.15"
  },
  "files": [
    "lib /**/*"
  ]
}
